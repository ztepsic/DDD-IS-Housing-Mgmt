<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ISHousingMgmt.Domain" namespace="ISHousingMgmt.Domain">
	<class name="City" table="Cities">
		<id name="Id">
			<generator class="native" />
		</id>
		<version name="Version" />
		<property name="PostalCode" not-null="true" unique="true"/>
		<property name="Name" not-null="true" />
	</class>
</hibernate-mapping>